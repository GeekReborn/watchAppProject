<view><view class="problem-container" style="{{('width:'+containerWidth+';padding-left:'+(containerPaddingLeft||containerPaddingX)+';padding-right:'+(containerPaddingRight||containerPaddingX))}}" scroll-y="true"><view class="problem-title" style="{{(width!=690?'width: '+width+'rpx;padding-left: 30rpx;':'')}}">{{Title}}</view><view class="problem-desc theme-color" style="{{(width!=690?'width: '+width+'rpx;padding-left: 30rpx;':'')}}">{{problemDescText}}</view></view><view class="uploader-list list-pd"><view class="uploader-list-cell cell-pd"><view class="uploader-container"><view class="uploader-body"><view class="uploader-files" style="{{(width!=690?'width: '+width+'rpx;padding-left: 30rpx;':'')}}"><block wx:if="{{images==''}}"><block><block wx:for="{{imageList}}" wx:for-item="images" wx:for-index="index" wx:key="index"><block><view class="uploader-file"><image class="uploader-img" src="{{images}}" data-src="{{images}}" data-event-opts="{{[['tap',[['previewImage',['$event']]]]]}}" bindtap="__e"></image></view></block></block></block></block><block wx:else><block><view class="uploader-file"><image class="uploader-img" src="{{images}}" data-src="{{images}}" data-event-opts="{{[['tap',[['previewImage',['$event']]]]]}}" bindtap="__e"></image></view></block></block></view></view></view></view></view></view>